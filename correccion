function get_cal_date() {
  const reprocess_date = dataform.projectConfig.vars.reprocess_date;
  if (reprocess_date && reprocess_date.trim() !== "") {
    return reprocess_date;
  }
  const yesterday = new Date(Date.now() - 24 * 3600 * 1000);
  return yesterday.toISOString().slice(0, 10);
}

module.exports = {
  get_cal_date
};
